{
  "name": "digitree",
  "version": "0.0.0",
  "author": {
    "name": "Celsiuss"
  },
  "description": "A Digimon evolution helper.",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron:dev": "concurrently \"ng serve\" \"electron . --dev\"",
    "electron:build": "rimraf build dist && ng build -c electron && electron-builder --win --linux"
  },
  "main": "electron/app.js",
  "private": true,
  "build": {
    "directories": {
      "output": "build"
    },
    "appId": "digitree",
    "files": [
      "dist/digitree/**/*",
      "electron/app.js",
      "!node_modules/**/*"
    ],
    "linux": {
      "target": "AppImage"
    },
    "win": {
      "target": "portable"
    }
  },
  "dependencies": {
    "@angular/animations": "^16.2.7",
    "@angular/cdk": "^16.2.6",
    "@angular/common": "^16.2.7",
    "@angular/compiler": "^16.2.7",
    "@angular/core": "^16.2.7",
    "@angular/forms": "^16.2.7",
    "@angular/material": "^16.2.6",
    "@angular/platform-browser": "^16.2.7",
    "@angular/platform-browser-dynamic": "^16.2.7",
    "@angular/router": "^16.2.7",
    "csv-parse": "^5.5.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.4",
    "@angular/cli": "^16.2.4",
    "@angular/compiler-cli": "^16.2.7",
    "@angular/language-service": "^16.2.7",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.2",
    "concurrently": "^8.2.1",
    "electron": "^26.2.4",
    "electron-builder": "^24.6.4",
    "electron-debug": "^3.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "rimraf": "^5.0.5",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.9.5"
  }
}
